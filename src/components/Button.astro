---
interface Props {
  label?: string;
  url?: string;
}
const { label = "Label", url = "#" } = Astro.props;
---

<a
  href={url}
  class="bg-slate-600 text-slate-100 border-transparent px-8 py-2 mt-4 rounded hover:bg-white hover:text-slate-600 hover:border-slate-600 border-2 transition-colors"
  >{label}</a
>
